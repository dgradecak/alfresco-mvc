<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>com.gradecak.alfresco</groupId>
    <artifactId>alfresco-parent-distribution</artifactId>
    <version>2.0.0-SNAPSHOT</version>
    <relativePath>../alfresco-parent-distribution/pom.xml</relativePath>
  </parent>

  <artifactId>alfresco-mvc</artifactId>
  <name>alfresco-mvc</name>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>

  <dependencies>
    <dependency>
      <groupId>${alfresco.groupId}</groupId>
      <artifactId>alfresco-data-model</artifactId>
      <scope>compile</scope>
    </dependency>

    <dependency>
      <groupId>${alfresco.groupId}</groupId>
      <artifactId>alfresco-repository</artifactId>
      <scope>compile</scope>
    </dependency>

  </dependencies>

  <repositories>
    <repository>
      <id>gradecak-public</id>
      <url>http://gradecak.com/repository/releases</url>
    </repository>
    <repository>
      <id>gradecak-public-snapshots</id>
      <url>http://gradecak.com/repository/snapshots</url>
      <snapshots>
        <enabled>true</enabled>
        <updatePolicy>daily</updatePolicy>
      </snapshots>
    </repository>
  </repositories>

</project>
